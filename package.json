{
  "name": "56k-knowledge-hub",
  "version": "0.9.0",
  "description": "AI-powered knowledge assistant for 56k Agency",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "frontend": "npx http-server ./public -p 8080 -c-1",
    "dev:all": "concurrently -k \"npm:dev\" \"npm:frontend\"",
    "start:all": "concurrently -k \"npm:start\" \"npm:frontend\"",
    "setup": "npm install && node setup.js",
  "test": "node tools/test-connections.js",
  "test:ai": "node tools/test-ai-engine.js",
  "test:claude": "node tools/test-connections.js --claude",
  "test:google": "node tools/test-connections.js --google",
  "test:clickup": "node tools/test-connections.js --clickup",
  "test:db": "node tools/test-connections.js --db",
  "test:server": "node tools/test-connections.js --server",
    "build": "echo Build step placeholder",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "format": "prettier . --write",
    "check:types": "echo 'No TS yet'",
    "quality": "npm run lint && npm test",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "docker:build": "docker build -t 56k-knowledge-hub .",
    "docker:run": "docker run -p 3000:3000 --env-file .env 56k-knowledge-hub",
    "docker:compose": "docker-compose up -d",
    "clean": "rm -rf node_modules package-lock.json && npm install",
    "release:patch": "npm version patch -m 'chore: bump version to %s'",
    "release:minor": "npm version minor -m 'chore: bump version to %s'",
    "release:major": "npm version major -m 'chore: bump version to %s'",
    "release:prep": "node scripts/prepare-release.js"
  },
  "keywords": [
    "ai",
    "claude",
    "clickup",
    "google-drive",
    "knowledge-management"
  ],
  "author": "56k Agency",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "axios": "^1.6.0",
    "google-auth-library": "^9.0.0",
  "sqlite3": "^5.1.6",
    "body-parser": "^1.20.2"
  ,"pdf-parse": "^1.1.1",
  "mammoth": "^1.4.19",
  "exceljs": "^4.3.0",
  "unzipper": "^0.10.11",
  "xml2js": "^0.4.23",
  "openai": "^4.56.0"
  ,"archiver": "^6.0.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "http-server": "^14.1.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}