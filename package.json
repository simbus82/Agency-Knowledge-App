{
  "name": "56k-knowledge-hub",
  "version": "1.0.0",
  "description": "AI-powered knowledge assistant for 56k Agency",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "frontend": "npx http-server ./public -p 8080 -c-1",
    "setup": "npm install && node setup.js",
    "test": "node test-connections.js",
    "test:ai": "node test-ai-engine.js",
    "test:claude": "node test-connections.js --claude",
    "test:google": "node test-connections.js --google",
    "test:clickup": "node test-connections.js --clickup",
    "test:db": "node test-connections.js --db",
    "test:server": "node test-connections.js --server",
    "build": "echo 'Build completed - static files ready'",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "docker:build": "docker build -t 56k-knowledge-hub .",
    "docker:run": "docker run -p 3000:3000 --env-file .env 56k-knowledge-hub",
    "docker:compose": "docker-compose up -d",
    "clean": "rm -rf node_modules package-lock.json && npm install"
  },
  "keywords": [
    "ai",
    "claude",
    "clickup",
    "google-drive",
    "knowledge-management"
  ],
  "author": "56k Agency",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "axios": "^1.6.0",
    "google-auth-library": "^9.0.0",
  "sqlite3": "^5.1.6",
    "body-parser": "^1.20.2"
  ,"pdf-parse": "^1.1.1",
  "mammoth": "^1.4.19",
  "exceljs": "^4.3.0",
  "unzipper": "^0.10.11",
  "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "http-server": "^14.1.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}